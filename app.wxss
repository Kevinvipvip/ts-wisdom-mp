/**app.wxss**/
.page {
    min-height: 100vh;
    background-color: rgb(239, 239, 239);
    overflow: hidden;
    padding-top: 93rpx;
}

image {
    display: block;
    width: 100%;
    height: 100%;
}

.white {
    background-color: #ffffff;
}
/* 超出省略 start */
.one-line-ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: nowrap;
}

.two-line-ellipsis {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

/* 超出省略 end */
